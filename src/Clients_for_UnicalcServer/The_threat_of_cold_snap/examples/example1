(*Угроза похолодания*)
tsr[0] = -10.91;
tsr[1] = -15.5;
tsr[2] = -8;
tsr[3] = 2;
tsr[4] = 2;
tsr[5] = 2;
tsr[6] = 2;
tsr[7] = 11;
tsr[8] = 10;
tsr[9] = 2;
tsr[10] = -8.5;
tsr[11] = -15.5;
i = 1;
tvn = 18;
dt[1] = 10;
bnt = 22000;
dbt = [-20, 20];
dtk = 1920;
dtt = [0,5000];
bdeft = 0;
bne = [18000, 23000];
dbe = [-10, 10];
det = [0,2500];
bdefe = 0;
dbt= dt[i]/( tvn- tsr[i]);
dbe=0.3*dbt;
bdeft= bnt* dbt - dtk - dtt;
bdefe= bne* dbe - det;
